<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sty.ne.rxjavastudy">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BasicConceptActivity"
            android:label="基础概念"/>
        <activity
            android:name=".CreationOperatorActivity"
            android:label="创建型操作符"/>
        <activity
            android:name=".TransformationOperatorActivity"
            android:label="变换型操作符"/>
        <activity
            android:name=".FilterOperatorActivity"
            android:label="过滤型操作符"/>
        <activity
            android:name=".ConditionOperatorActivity"
            android:label="条件型操作符"/>
        <activity
            android:name=".MergeOperatorActivity"
            android:label="合并型操作符"/>
        <activity
            android:name=".ExceptionOperatorActivity"
            android:label="异常处理型操作符"/>
        <activity
            android:name=".ThreadSwitchActivity"
            android:label="线程切换"/>

    </application>

</manifest>